                <label for="password ">password:</label>
                <input type="password" id="password " placeholder="password " minlength="4 " required name="password" />

                <input type="submit" id="submit ">

            </form>
            <p>don" t you have account</p>
            <button class="signUP"><a href="signUp.ejs">sign up</a></button>
        </div>
    </div>
